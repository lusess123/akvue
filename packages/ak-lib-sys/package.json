    "url": "https://github.com/lusess123/akvue/issues"
  },
  "homepage": "https://github.com/lusess123/akvue/packages/ak-lib-sys#readme",
  "dependencies": {
    "axios": "*",
    "iview": "^2.9.0",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "path": "^0.12.7",
    "rxjs": "^5.5.6",
    "vue": "^2.5.13",
    "vue-axios": "2.0.2"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "shx": "^0.2.2",
    "typescript": "^2.6.2"
  }
