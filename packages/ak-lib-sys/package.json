{
  "name": "ak-lib-sys",
  "version": "0.0.1",
  "description": "前端框架的核心模块",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": " tsc",
    "clean_js": " find .  -name  \"*.js\"  -o -name \"*.d.ts\" -o -name \"com\"  -o -name \"event\"  -o -name \"net\" -o -name \"js\"   |grep -v \"src/\" |grep -v \"node_modules/\" | xargs rm -rf",
    "deljs": "shx rm -rf js",
    "copy": "shx mv  ./js/*  .",
    "copyvue": "cd src;find ./  -name \"*.vue\" | xargs -I{} rsync -R \"{}\" ../js/;cd ..;",
    "build": "npm-run-all  clean_js tsc  copyvue copy deljs "
  },
  "repository": {
    "type": "git",
    "url": "git+ https://github.com/lusess123/akvue.git"
  },
