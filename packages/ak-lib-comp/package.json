{
  "name": "ak-lib-comp",
  "version": "0.0.1",
  "description": "前端框架的核心模块",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": " tsc",
    "clean_js": " find .  -name  \"*.js\"  -o -name \"*.d.ts\" -o -name \"buttons\"  -o -name \"complex\"  -o -name \"iview\"-o -name \"multi\" -o -name \"inputtag\" -o -name \"js\" -o -name \"multi\"  -o -name \"iview\"   |grep -v \"src/\" |grep -v \"node_modules/\" | xargs rm -rf",
    "deljs": "shx rm -rf js",
    "copy": "shx mv  ./js/*  .",
    "copyvue": "cd src;find ./  -name \"*.vue\" | xargs -I{} rsync -R \"{}\" ../js/;cd ..;",
    "build": "npm-run-all   tsc  copyvue copy deljs ",
    "rebuild": "npm-run-all  clean_js build "
  },
  "repository": {
    "type": "git",
    "url": "git+ https://github.com/lusess123/akvue.git"
  },
  "author": "zyk2003xxx@163.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lusess123/akvue/issues"
  },
  "homepage": "https://github.com/lusess123/akvue/packages/ak-lib-comp#readme",
  "dependencies": {
    "ak-lib-sys":"*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.2",
    "shx": "^0.2.2",
    "typescript": "*"
  }
}
 