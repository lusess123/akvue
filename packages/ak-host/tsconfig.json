{
  "compilerOptions": {
    "experimentalDecorators": true,
    "allowJs": true,
    "target": "es6",
    "module": "es6",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    // 启用 vuex-class 需要开启此选项
    "strictFunctionTypes": false,
    "lib": [
      "es6",
      "dom"
    ],
    "declaration": false,
    "sourceMap": true,
    // "baseUrl": "./src/framework/",
    // "types": [
    //   "node",
    // ]
  }
}